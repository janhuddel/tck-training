{
  "name": "@tck-training/excel-parser",
  "version": "1.0.0-alpha.7",
  "description": "Diese Bibliothek stellt Funktionen bereit, um Trainingstermine aus einer Excel-Datei zu ermitteln (üblicherweise die Hallentermine für eine Winter-Runde).",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "excel",
    "parser",
    "typescript",
    "xlsx",
    "tennis",
    "training",
    "calendar",
    "hallentermine"
  ],
  "author": "Jan Rohwer <jan.rohwer@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tck-training/tck-training.git",
    "directory": "packages/excel-parser"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepublishOnly": "npm run build",
    "format": "npx prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "npx prettier --check \"**/*.{ts,js,json,md}\""
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "xlsx": "^0.18.5"
  }
}
