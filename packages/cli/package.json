{
  "name": "@tck-training/cli",
  "version": "1.0.0-alpha.8",
  "description": "Tool (CLI), um Trainingstermine aus einer Excel-Datei zu lesen und in verschiedenen Formaten auszugeben.",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "tck-training": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "tennis",
    "training",
    "calendar",
    "hallentermine",
    "ics",
    "cli"
  ],
  "author": "Jan Rohwer <jan.rohwer@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tck-training/tck-training.git",
    "directory": "packages/excel-parser"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@tck-training/excel-parser": "1.0.0-alpha.6",
    "commander": "^12.0.0",
    "console-table-printer": "^2.12.1",
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "tsx": "^4.7.1",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  }
}
